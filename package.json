{
  "version": "0.5.1",
  "name": "file-computed",
  "description": "文件型计算属性，当且仅当文件变化时才重新做计算",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dishait/file-computed.git"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "build": "unbuild",
    "dev": "pnpm run stub",
    "stub": "unbuild --stub",
    "play": "tsx src/index.ts",
    "test:update": "vitest --u",
    "test:watch": "vitest --watch",
    "coverage": "vitest --coverage",
    "prepublishOnly": "npm run build",
    "build:watch": "npm run build -- --watch src",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "keywords": [
    "lib",
    "tool",
    "node",
    "file",
    "utils",
    "computed"
  ],
  "author": {
    "name": "markthree",
    "email": "1801982702@qq.com",
    "url": "https://github.com/markthree"
  },
  "bugs": {
    "email": "1801982702@qq.com",
    "url": "https://github.com/dishait/file-computed/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "bumpp": "^8.2.1",
    "c8": "^7.12.0",
    "find-up": "^6.3.0",
    "mem": "^9.0.2",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2",
    "vitest": "^0.26.3"
  },
  "dependencies": {
    "destr": "^1.2.2",
    "fast-json-stringify": "^5.5.0",
    "m-type-tools": "^0.4.0",
    "ohash": "^1.0.0",
    "perfect-debounce": "^0.1.3",
    "unstorage": "^1.0.1"
  }
}
